{
    "name": "collinped/laravel-aimtell",
    "description": "Aimtell laravel integration",
    "keywords": [
        "aimtell",
        "aimtell-php",
        "laravel-aimtell"
    ],
    "homepage": "https://github.com/collinped/laravel-aimtell",
    "license": "MIT",
    "authors": [
        {
            "name": "Collin Pedersen",
            "email": "me@collinped.com",
            "homepage": "https://www.collinped.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Collinped\\Aimtell\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Collinped\\Aimtell\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Collinped\\Aimtell\\AimtellServiceProvider"
            ],
            "aliases": {
                "Aimtell": "Collinped\\Aimtell\\AimtellFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/collinped"
        }
    ]
}
